                                            <ImageView
                                                android:id="@+id/image_thirdView_actionOne_ac"
                                                android:layout_width="25dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/error" />

                                            <ImageView
