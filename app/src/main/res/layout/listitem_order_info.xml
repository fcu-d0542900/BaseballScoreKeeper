                                            <ImageView
                                                android:id="@+id/image_homeView_throwTwo"
                                                android:layout_width="25dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/throw6" />

                                            <ImageView
                                                android:id="@+id/image_homeView_throwRight"
                                                android:layout_width="25dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/throw_right" />
