                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/to" />

                                            <ImageView
                                                android:id="@+id/image_secondView_actionTwo"
                                                android:layout_width="25dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/throw5" />

                                            <ImageView
                                                android:id="@+id/image_secondView_actionTwo_ac"
                                                android:layout_width="25dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
