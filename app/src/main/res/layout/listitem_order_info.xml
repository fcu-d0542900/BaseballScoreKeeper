                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/to" />

                                            <ImageView
                                                android:id="@+id/image_thirdView_actionTwo"
                                                android:layout_width="25dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/throw5" />

                                            <ImageView
                                                android:id="@+id/image_thirdView_actionTwo_ac"
                                                android:layout_width="25dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/error" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:id="@+id/linear_thirdView_throw"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:orientation="horizontal"
                                            android:visibility="gone">

                                            <ImageView
                                                android:layout_width="25dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/throw_left" />

                                            <ImageView
                                                android:id="@+id/image_thirdView_throwOne"
                                                android:layout_width="25dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/throw9" />

                                            <ImageView
                                                android:layout_width="20dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/to" />

                                            <ImageView
                                                android:id="@+id/image_thirdView_throwTwo"
                                                android:layout_width="25dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                app:srcCompat="@drawable/throw6" />

                                            <ImageView
                                                android:layout_width="25dp"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
